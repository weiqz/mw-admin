import req from '../request/request'

export function getdivimange(params){
    return req({
        url:'/api/department/list',
        method:'get',
        params
    })
}

export function adddivimange(data){
    return req({
        url:'/api/department/add',
        method:'post',
        data
    })
}

export function editdivimange(data){
    return req({
        url:'/api/department/update',
        method:'post',
        data
    })
}

export function changeDmActive(params){
    return req({
        url:'/api/department/active',
        method:'get',
        params
    })
}