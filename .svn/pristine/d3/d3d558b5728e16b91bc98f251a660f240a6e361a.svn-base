@baseColor:#1abc9c;
@backgColor:#f3f3f3;

.el-table th, .el-table td{
    text-align: center;
}
tr.el-table__row .cell a{
    color:@baseColor;
    cursor: pointer;
    margin-right: 15px;
}
.sqsx{
    font-size:12px;
    color:#666;
    margin-right:10px;
    cursor: pointer;
}
.bold-font{
    font-weight: 700;
}
table{
    td{
        .cell{
            a{
                color: @baseColor;
                cursor: pointer;
                margin-right: 10px;
            }
        }
    }
}
.el-input {
  margin-left: -1px;
  width: 210px;
}
ul, 
li {
list-style: none;
}

.el-popover{
  position: absolute;
  left: 50% !important;
  background: #fff !important;
  width:200px !important;
  height: 200px;
  color: #000;
  line-height: 1.4;
  text-align: justify;
  font-size: 14px;
  box-shadow: none;
  border: 1px solid #ccc;
  word-break: break-all;
  cursor: pointer;
}
